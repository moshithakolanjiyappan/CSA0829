def find_min_rotated(arr):
    low, high = 0, len(arr) - 1
    
    while low < high:
        mid = (low + high) // 2
        
        if arr[mid] > arr[high]:
            low = mid + 1
        else:
            high = mid
            
    return arr[low]

# Example:
arr = [4, 5, 6, 7, 0, 1, 2]
print("Minimum element is:", find_min_rotated(arr))
